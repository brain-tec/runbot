<odoo>
  <data>
    
    <record id="branch_form" model="ir.ui.view">
        <field name="name">runbot.branch.form</field>
        <field name="model">runbot.branch</field>
        <field name="inherit_id" ref="runbot.branch_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='sticky']" position="after">
                <field name="coverage"/>
            </xpath>
        </field>
    </record>
          

  </data>
</odoo>