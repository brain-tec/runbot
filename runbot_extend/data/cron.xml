<odoo>
    <data noupdate="1">
        <record model="ir.cron" id="nightly_coverage_build_cron">
            <field name="name">Create pending build for coverage</field>
            <field name="model_id" ref="runbot.model_runbot_build"/>
            <field name="state">code</field>
            <field name="code">model._cron_create_coverage_build('hostname')</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="nextcall">2000-01-01 01:00:00</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
        </record>
    </data>
</odoo>
